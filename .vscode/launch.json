{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch with webpack",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "start"
        ],
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "showAsyncStacks": true
      },
    ],
  }
  